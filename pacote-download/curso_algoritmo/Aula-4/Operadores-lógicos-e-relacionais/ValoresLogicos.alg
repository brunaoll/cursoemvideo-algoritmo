algoritmo "valoreslogicos"
var
   A, B, C: Inteiro
inicio
      A <- 2
      B <- 3
      C <- 5
      Escreval(A>B)
      Escreval(B<C)
      Escreval(C<>A+B)
      Escreval(C>=A+B)
      Escreval((A=B) e (C>A))
      Escreval((B=C-A) e (A<>C+B))
      Escreval((A=B) ou (C>A))
      Escreva(nao((A=B) ou (C>A)))
fimalgoritmo
