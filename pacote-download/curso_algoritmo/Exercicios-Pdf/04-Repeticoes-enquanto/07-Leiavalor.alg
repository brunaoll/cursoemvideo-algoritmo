algoritmo "Leiavalor"
var
   p, u, i: Inteiro
inicio
      Escreva("Digite o primeiro valor: ")
      Leia(p)
      Escreva("Digite o ultimo valor: ")
      Leia(u)
      Escreva("Digite o incremento: ")
      Leia(i)
      Enquanto (p <= u) faca
               Escreva(p)
               p <- p + i
      FimEnquanto
      Escreva(" Acabou!")
fimalgoritmo
