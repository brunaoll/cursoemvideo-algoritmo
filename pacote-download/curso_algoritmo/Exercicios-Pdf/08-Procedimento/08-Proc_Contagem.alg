algoritmo "Proc_Contagem"
var
   I, F, In, C: Inteiro
Procedimento Contador()
inicio
      Para C <- I ate F passo In faca
           Escreva(C, " >>")
      FimPara
      Escreva(" FIM")
FimProcedimento
inicio
      Escreva("Inicio: ")
      Leia(I)
      Escreva("Fim: ")
      Leia(F)
      Escreva("Incremento: ")
      Leia(In)
      Contador()
fimalgoritmo

